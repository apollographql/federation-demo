query {
  me {                    -> Accounts
    id                    -> Accounts
    name                  -> Accounts
    username              -> Accounts
    reviews {             -> Reviews
      id                  -> Reviews
      body                -> Reviews
      author {            -> Review
        id                -> Accounts
        name              -> Accounts
        username          -> Accounts/ === Reviews
        reviews {         -> Reviews
          id              -> Reviews
          body            -> Reviews [ Leave it Review]
          product {       -> Reviews
            name          -> Accounts
            price         -> Accounts
            weight        -> Accounts
          }
        }
      }
      product {           -> Reviews
        upc               -> Accounts
        name              -> Accounts
        inStock           -> Reviews
        shippingEstimate  -> Reviews
        weight            -> Accounts
        price             -> Accounts
        reviews {         -> Reviews
          id              -> Reviews
          body            -> Reviews
          author {        -> Reviews
            id            -> Accounts
            name          -> Accounts
            username      -> Accounts
          }
        }
      }
    }
  }
}

